@startuml
skinparam monochrome true
skinparam packageStyle rectangle
skinparam shadowing false
'skinparam linetype ortho
'left to right direction
skinparam classAttributeIconSize 0
hide circle
hide methods

title Domain Model us006

class FM {
    name
    vatNumber
    website
    phoneNumber
    email
}

class JobType {
    reference
    description
    informalDesc
    technicalDesc
    duration
    cost
}



class Vehicle {
    name
    email
    position
    phone
}

class VehicleType {
    shortDescription
    fullDescription
}




Vehicle "1" - "*" VehicleType: has >
JobType "1" -- "*" FM : creates >
FM "1" -- "*" Vehicle : has >



@enduml